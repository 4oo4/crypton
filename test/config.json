{
  "note": "This exists to specify the port so we can run the server without escalated privilege",
  "port": 1025,
  "database": {
    "type": "postgres",
    "username": "crypton_test_user",
    "password": "crypton_test_user_password",
    "database": "crypton_test",
    "host": "localhost",
    "port": 5432
  },
  "defaultKeySize": 32,
  "id_translator": {
    "key_file": "/dev/urandom"
  },
  "cookieSecretFile": "/dev/urandom",
  "privateKey": "privateKeyExample.pem",
  "certificate": "certificateExample.pem",
  "appPath": ".",
  "redis": {
    "host": "localhost",
    "port": 6379,
    "pass": ""
  },
  "commitStatusCheckDelay": 100,
  "commitStatusCheckLimit": 100,
  "securityHeaders": {
    "csrf": true,
    "csp": {
      "default-src": "self",
      "script-src": "self",
      "img-src": "self",
      "style-src": "self",
      "font-src": "self",
      "object-src": "none"
    },
    "xframe": "SAMEORIGIN"
  }
}
